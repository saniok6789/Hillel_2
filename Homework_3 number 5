class CustomList:
    def __init__(self, items):
        self.items = items

    def __len__(self):
        return len(self.items)

    def __iter__(self):
        return iter(self.items)

    def __getitem__(self, index):
        return self.items[index]

# Тестування
cl = CustomList([1, 2, 3, 4])
print(len(cl))  # 4
print(sum(cl))  # 10
print(min(cl))  # 1
